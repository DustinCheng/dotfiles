[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --graph --pretty='%C(yellow)%h%Creset %C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)%an%Creset'
	type = cat-file -t
	dump = cat-file -pst = status
	dt = difftool
	da = difftool --dir-diff
[diff]
	tool = p4merge
[merge]
	tool = p4merge
[difftool]
	prompt = false
[difftool "p4merge"]
	path = "/Applications/p4merge.app/Contents/MacOS/p4merge"
[mergetool]
	prompt = false
[mergetool "p4merge"]
	path = "/Applications/p4merge.app/Contents/MacOS/p4merge"
