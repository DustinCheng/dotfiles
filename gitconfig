[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --oneline --all --decorate --graph --since=90.days --pretty=format:"%C(yellow)%h%C(auto)%d%Creset %s %Cgreen%cr %C(bold blue)%an%Creset" 
	type = cat-file -t
	dump = cat-file -pst = status
	dt = difftool
	da = diffall
[diff]
    tool = p4merge
[merge]
    tool = p4merge
[difftool]
	prompt = false
[mergetool]
	prompt = false
