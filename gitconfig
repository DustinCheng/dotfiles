[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -pst = status
	dt = difftool
	da = diffall
[diff]
    tool = p4merge
[merge]
    tool = p4merge
[difftool]
	prompt = false
[mergetool]
	prompt = false
